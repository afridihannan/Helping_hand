{
    "name":"Helping Hand",
    "version":"0.0.1",
    "manifest_version": 2,
    "browser_action":{
        "default_popup":"./popup.html"
    },
    "permissions":["tabs","<all_urls>"],
    "content_scripts":[
        {
            "matches":["http://*/*","https://*/*"],
            "run_at":"document_end",
            "js":["./jquery-ui-1.12.1/jquery-3.6.0.js","./jquery-ui-1.12.1/jquery-ui.js","features.js"],
            "css":[
                "./jquery-ui-1.12.1/jquery-ui.css",
                "./jquery-ui-1.12.1/jquery-ui.structure.css",
                "./jquery-ui-1.12.1/jquery-ui.theme.css"
            ]
        }
    ],
    "background":{
        "scripts":["background.js"]
    }

}